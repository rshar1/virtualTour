<html>


<head>
	<script src = "http://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>

	<script type = "text/javascript">
			var c;
			var ctx;
			var x;
			var y;
			var imgL;
			var img;
			var imgR;
			
//Modify the following variables once the values are known

			var maxA = 7;			//The number of images for each position,starting at 0
			var scale = 2000/1600;			//The scale will determine how big the images are
		
		
// p will represent the position in the museum and a will represent the angle you are facing at a given time
			var p = 0;
			var a = maxA + 1;

			$(document).ready(function(){
			c = document.getElementById("myCanvas");
			ctx = c.getContext("2d");
			maxA = 7;
			scale = 2000 / 1600;
			p=0;
			a= maxA+1;
			x = 0;
			y = 0;
			imgL = new Image();
			img = new Image();
			imgR = new Image();
			
		/*
			imgR.src = "data:image/png;base64,";
			img.src = "data:image/jpeg;base64,";
		*/
		
		//This finds the first images that should be used and loads them
			
			imgL.src = "hats_" + p + "_" + ((a-1) % (maxA + 1) )+".jpg";
			img.src = "hats_" + p + "_" + (a % (maxA+1))+".jpg";
			imgR.src = "hats_" + p + "_" + ((a+1) % (maxA + 1))+".jpg";
			
			
			
			
			
			
		//Draw the images when the page loads
			
			ctx.drawImage(imgL,x-img.width * scale,y,img.width* scale,img.height * scale);
						ctx.drawImage(img,x,y,img.width * scale,img.height * scale);
						ctx.drawImage(imgR,x+img.width * scale,y,img.width * scale,img.height * scale);
		

					
				
					$(document).keydown(function(event){
						
						
						var c = document.getElementById("myCanvas");
						var ctx = c.getContext("2d");
						
			//Erase the page when the user presses a button on the keyboard
			
						ctx.clearRect(0,0,2000,1000);
					//Left arrow
						if(event.which == 37){
							x+= 20;
						}
					//up arrow
						if(event.which == 38){
							//y+=20;
						}
					//right arrow
						if(event.which == 39){
							x-=20;
						}
					//down arrow
						if(event.which == 40){
							//y-=20;
						}
						
					//Recenter the images if they move too much to the left side
						if(x < -((img.width * scale) * 0.2)){
							imgL.src = img.src;
							img.src = imgR.src;
							a++;
							imgR.src = "hats_" + p + "_" + ((a+1) % (maxA + 1))+".jpg";
							x += img.width * scale;
						}
					//Recenter the images if they move too much to the right side
						if(x > (img.width * scale ) *0.2 ){
							imgR.src = img.src;
							img.src = imgL.src;
							a--;
							if(a < 1) a = maxA;  //This ensures that you never have a negative value for a
							imgL.src = "hats_" + p + "_" + ((a-1) % (maxA + 1))+".jpg";
							x -= img.width * scale;
						}
						
				//Draw the images after their positions have been modified
						ctx.drawImage(imgL,x-img.width * scale,y,img.width* scale,img.height * scale);
						ctx.drawImage(img,x,y,img.width * scale,img.height * scale);
						ctx.drawImage(imgR,x+img.width * scale,y,img.width * scale,img.height * scale);
						
					})
				})
		

			

		

	</script>
</head>




<body>

	
	<canvas id="myCanvas" width="2000" height="1500" style="border:1px solid #000000;"> 
		
	</canvas>


	
	

</body>




</html>
